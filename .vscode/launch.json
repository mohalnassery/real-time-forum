{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "env": {
                "TLS_CERT_FILE": "${workspaceFolder}/server/cert/server.crt",
                "TLS_KEY_FILE": "${workspaceFolder}/server/cert/server.key"
            },
            "program": "${workspaceFolder}/server/main.go"
        }
    ]
}